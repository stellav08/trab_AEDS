class Comanda {
private int numeroComanda;
private String consumo; 
private double valor;
private int quant_cliente;

public void getComanda(String consumo, double valor, int numeroComanda){
    this.consumo = consumo;
    this.valor = valor;
    this.numeroComanda =  numeroComanda;
}

public void mostrarComanda() {
        System.out.println("NÃºmero da Comanda: " + numeroComanda);
        System.out.println("Consumo: " + consumo);
        System.out.println("Valor: " + valor);
    }
     public int getNumeroComanda() {
        return numeroComanda;
    }
    
    public double calculo10p(){
        double dez = 1.1 * valor;
        return dez;
    }

    public double valor(){
        return valor;
    }
}

